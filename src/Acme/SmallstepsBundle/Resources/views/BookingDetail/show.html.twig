{% extends '::base.html.twig' %}

{% block body -%}
    <h1>BookingDetail</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Bookeddate</th>
                <td>{{ entity.bookedDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Breakfastclub</th>
                <td>{{ entity.breakfastClub }}</td>
            </tr>
            <tr>
                <th>Afterdayclub</th>
                <td>{{ entity.afterdayClub }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_booking') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_booking_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
